{
  "version": 2,
  "buildCommand": "vite build && tsup",
  "installCommand": "npm install",
  "outputDirectory": "dist/public",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api"
    },
    {
      "source": "/((?!api|assets).*)",
      "destination": "/index.html"
    }
  ]
}
